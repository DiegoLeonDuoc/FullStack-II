{
  "builds": [{ "src": "src/**/*", "use": "@vercel/static" }],
  "routes": [
    { "src": "/styles/(.*)", "dest": "/src/styles/$1" },
    { "src": "/scripts/(.*)", "dest": "/src/scripts/$1" },
    { "src": "/", "dest": "/src/pages/index.html" },
    { "src": "/(.*)", "dest": "/src/pages/index.html" }
  ]
}
